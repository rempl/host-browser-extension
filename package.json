{
  "name": "host-browser-extension",
  "version": "1.0.2",
  "description": "Rempl host for broswer Developer Tools",
  "author": "Roman Dvornov",
  "license": "MIT",
  "repository": "rempl/host-browser-extension",
  "type": "module",
  "keywords": [
    "rempl",
    "host"
  ],
  "dependencies": {
    "rempl": "1.0.0-alpha.22"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.188",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "jszip": "^3.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "lint": "eslint src/*.{cjs,ts} scripts/*.cjs",
    "tscheck": "tsc --noEmit",
    "dev": "node ./scripts/build.cjs",
    "build": "NODE_ENV=production node ./scripts/build.cjs && node ./scripts/zip.cjs"
  }
}
