{
    "include": ["src/*.ts"],
    "compilerOptions": {
        "target": "ES2020",
        "module": "node16",
        "moduleResolution": "nodenext",
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "allowJs": false,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noEmit": true
    }
}
